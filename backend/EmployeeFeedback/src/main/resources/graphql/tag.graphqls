type Tag {
	tagId: ID!
	name: String!
	color: String!
	totalMessages: Long
	createdBy: Employee
}

input TagInput {
	employeeId: Long
	name: String
	color: String
}

input DeleteTagInput {
	tagId: Long
	
}

extend type Mutation {
	newTag(input: TagInput) : Tag!
	removeTag(tagId: Long!): Tag!

}

extend type Query {
	findAllTags: [Tag]!
	
}